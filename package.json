{
  "name": "svelte-modals",
  "version": "2.0.0",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-package",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "release": "npm run package && npm publish --access public",
    "release:next": "npm run package && npm publish --access public --tag next"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@mattjennings/heroicons-svelte": "^0.0.6",
    "@sveltejs/adapter-vercel": "^4.0.0",
    "@sveltejs/kit": "^2.5.27",
    "@sveltejs/package": "^2.1.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "autoprefixer": "^10.4.14",
    "canvas-confetti": "^1.4.0",
    "cssnano": "^5.0.15",
    "eslint": "^9.13.0",
    "eslint-plugin-svelte": "^2.46.0",
    "fast-glob": "^3.2.11",
    "mdsvex": "^0.11.0",
    "mdsvexamples": "^0.3.3",
    "postcss": "^8.4.24",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.2.6",
    "rehype-autolink-headings": "^5.1.0",
    "rehype-slug": "^4.0.1",
    "remark-abbr": "^1.4.1",
    "remark-github": "^10.1.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "svelte-preprocess": "^6.0.0",
    "svelte2tsx": "^0.6.16",
    "tailwindcss": "^3.0.0",
    "tslib": "^2.6.0",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.4"
  },
  "peerDependencies": {
    "svelte": ">=5"
  },
  "type": "module",
  "keywords": [
    "svelte",
    "modal",
    "stack",
    "manager"
  ],
  "homepage": "https://svelte-modals.mattjennings.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattjennings/svelte-modals.git"
  },
  "license": "MIT",
  "author": {
    "name": "Matt Jennings",
    "url": "https://github.com/mattjennings"
  }
}
